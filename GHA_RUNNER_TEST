- name: GHA_TESTGAMMA
  uses: aws-actions/aws-codebuild-run-build@v1
  with:
    project-name: GHA_TESTGAMMA
    disable-source-override: true
    buildspec-override:   |
      version: 0.2
      phases:
        install:
          runtime-versions:
            nodejs: 16
          commands:
            - npm install -g typescript
            - npm install
        pre_build:
          commands:
            - echo Installing source NPM dependencies...
        build:
          commands:
            - echo Build started on `date`
            - tsc
            - npm prune --production
            - echo Test Webhook
          post_build:
            commands:
              - echo Build completed using account level credentials
